<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ToyProject.account.AccountRepository">


  <insert id="insertAccount" parameterType="com.ToyProject.user.vo.TestVo">
  INSERT INTO USERS 
  (USER_NO, USER_ID, PASSWORD)
  VALUES
  (#{USER_NO},#{USER_ID},#{PASSWORD})
  </insert>



</mapper>